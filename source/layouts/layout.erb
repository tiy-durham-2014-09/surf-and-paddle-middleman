<!doctype html>
<html>
  <head>
    <meta charset="utf-8">

    <!-- Always force latest IE rendering engine or request Chrome Frame -->
    <meta content="IE=edge,chrome=1" http-equiv="X-UA-Compatible">

    <!-- Use title if it's in the page YAML frontmatter -->
    <title><%= current_page.data.title || "CLOAK &amp; DAGGER CO." %></title>

    <!-- Uncomment the following if you want to use normalize -->
    <%= stylesheet_link_tag "normalize" %>

    <%= stylesheet_link_tag "main-body" %>

    <%= stylesheet_link_tag "header" %>

    <%= stylesheet_link_tag "profile" %>

    <!-- <%#= stylesheet_link_tag "gridtest" %> -->

    <link href='http://fonts.googleapis.com/css?family=Oswald:200,300,400' rel='stylesheet' type='text/css'>
    <link href='http://fonts.googleapis.com/css?family=Open+Sans:100,200,300,400,500,600' rel='stylesheet' type='text/css'>
    <link href="http://maxcdn.bootstrapcdn.com/font-awesome/4.2.0/css/font-awesome.min.css" rel="stylesheet">

  </head>

  <body class="<%= page_classes %>">
    <%= partial "header" %>
    <%= yield %>
  </body>

</html>
